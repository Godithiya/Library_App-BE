generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model books {
  id               Int      @id @default(autoincrement())
  title            String
  author           String
  genre            String
  publication_year Int
  stock            Int
  created_at       DateTime @default(now())
  updated_at       DateTime @updatedAt
}
